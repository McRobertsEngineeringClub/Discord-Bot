{
  "name": "mcroberts-engineering-club-discord-bot",
  "version": "1.0.0",
  "description": "Discord bot for McRoberts Engineering Club with role management, announcements, and Google Sheets integration",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node cleanup-and-register.js && node index.js",
    "register": "node cleanup-and-register.js",
    "cleanup": "node cleanup-and-register.js --cleanup-only",
    "emergency": "node cleanup-and-register.js --emergency-cleanup",
    "dev": "nodemon index.js"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^128.0.0",
    "nodemailer": "^6.9.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": ["discord", "bot", "engineering", "club", "roles", "announcements"],
  "author": "Tadjell Lyle",
  "license": "MIT"
}
